<div class='mt-4'>
    <div class='row'>

        {{#each data}}
            <div class='col-sm-6 col-lg-4'>
                <div class='list-game'>
                    <img
                        src='{{this.image}}'
                        class='card-img-top'
                        alt='{{this.image}}'
                    />
                    <div class='card-body'>
                        <h5 class='card-title'>{{this.name}}</h5>
                        <p class='card-text'>{{this.des}}</p>
                        <a href='#' class='btn btn-primary'>{{this.id}}</a>
                    </div>
                </div>
            </div>
        {{/each}}

    </div>
</div>